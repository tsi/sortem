{
  "name": "sortem",
  "productName": "Sort'em",
  "version": "0.2.2",
  "description": "Easily sort big photo collections",
  "private": true,
  "main": "src/electron/main.js",
  "scripts": {
    "sortem": "npm start",
    "dev": "APP_DEV=true npm start",
    "start": "concurrently -k \"BROWSER=none react-scripts start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons-kit": "^2.0.0",
    "react-scripts": "5.0.1",
    "sass": "^1.58.0",
    "sharp": "^0.31.3",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^22.2.0",
    "wait-on": "^7.0.1"
  }
}
